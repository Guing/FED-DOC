{
  "name": "admin-fe",
  "version": "1.0.3",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "dev-no-mock": "cross-env UMI_ENV=dev-no-mock umi dev",
    "build-dev": "cross-env UMI_ENV=prd-dev umi build",
    "build": "cross-env UMI_ENV=production umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-layout": "^5.0.19",
    "@umijs/preset-react": "1.x",
    "@umijs/route-utils": "^2.1.1",
    "@umijs/test": "^3.2.22",
    "antd": "^4.x",
    "axios": "^0.20.0",
    "date-fns": "^2.16.1",
    "dva-core": "^1.1.0 | ^1.5.0-0 | ^1.6.0-0",
    "echarts": "^4.9.0",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.20",
    "moment": "^2.29.3",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": ">=5.0.0 <6.0.0",
    "redbox-react": "1.x",
    "redux": "4.x",
    "umi": "^3.2.22",
    "uuid": "^8.3.1",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2"
  }
}
